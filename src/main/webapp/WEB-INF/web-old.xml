<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <filter>
        <filter-name>Charset</filter-name>
        <filter-class>step.learning.filters.CharsetFilter</filter-class>
    </filter>
    <filter>
        <filter-name>LogFilter</filter-name>
        <filter-class>step.learning.filters.LogFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Charset</filter-name>
        <url-pattern>*</url-pattern>
    </filter-mapping>

    <filter-mapping>
        <filter-name>LogFilter</filter-name>
        <url-pattern>*</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>step.learning.servlets.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateTextFileServlet</servlet-name>
        <servlet-class>step.learning.servlets.CreateTextFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern></url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateTextFileServlet</servlet-name>
        <url-pattern>/CreateTextFileServlet</url-pattern>
    </servlet-mapping>
</web-app>
